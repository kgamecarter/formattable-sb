<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Authors>Mariusz Stępień</Authors>
    <Description>A mutable FormattableString</Description>
    <PackageProjectUrl>https://github.com/mariusz96/formattablesb</PackageProjectUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/mariusz96/formattablesb</RepositoryUrl>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <PackageId>FormattableSb</PackageId>
    <Version>1.0.0</Version>
    <Copyright>Mariusz Stępień</Copyright>
    <PackageTags>formattablestringbuilder;mutable;formattablestring;formattable;string;strings;text;texts;builder;building;composite;format;formatting;interpolatedstring;interpolated;interpolation;expression;expressions;placeholder;placeholders;item;items;index;alignment;escaping;brace;braces</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
